# Function to generate the dictionary
def generate_square_dict(n):
    square_dict = {}
    for i in range(1, n + 1):
        square_dict[i] = i * i
    return square_dict

# Input from the user
n = int(input("Enter an integer (n): "))

# Generate the dictionary
result_dict = generate_square_dict(n)

# Print the dictionary
print(result_dict)
